{
  "name": "plex-playlist-manager",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./dist/* && rm -rf ./.cache/*",
    "start": "parcel -p 4000 src/index.html",
    "dev": "parcel -p 4000 src/index.html",
    "build": "rm -rf docs; parcel build src/index.html --public-url . --out-dir docs/",
    "postbuild": "cp -r public/* docs/"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "sass": "^1.26.5",
    "svelte": "^3.49.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "core-js": "3",
    "interactjs": "^1.9.17",
    "jquery": "^3.5.1",
    "moment": "^2.25.3",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "svelte-infinite-scroll": "^1.4.0",
    "svelte-preprocess": "^3.7.4"
  }
